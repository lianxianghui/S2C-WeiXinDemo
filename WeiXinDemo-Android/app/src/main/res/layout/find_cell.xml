<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/cell"
	android:alpha="1"
	android:background="@drawable/bg_normalcolor_ffffffff_pressedcolor_80ffffff"
	android:layout_height="40.0dp"
	android:layout_width="match_parent">

	<android.view.View
		android:id="@+id/bottom_line"
		android:alpha="1"
		android:background="#FFD8D8D8"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		android:layout_height="0.5dp"
		android:layout_width="0dp" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/left_image"
		android:alpha="1"
		app:srcCompat="@drawable/find_find_cell_left_image"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="18.0dp"
		android:layout_width="20.0dp"
		android:layout_height="20.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/name"
		android:alpha="1"
		android:background="#00FFFFFF"
		android:text="@string/find_fragment_find_cell_name"
		android:fontFamily="sans-serif"
		android:textColor="#FF1B1B1B"
		android:textSize="14sp"
		app:layout_constraintStart_toEndOf="@+id/left_image"
		android:layout_marginStart="16.0dp"
		android:layout_height="23.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		android:layout_width="wrap_content" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/right_icon"
		android:alpha="1"
		app:srcCompat="@drawable/find_find_cell_right_icon"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="15.0dp"
		android:layout_width="8.0dp"
		android:layout_height="13.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/right_head_image"
		android:alpha="1"
		app:srcCompat="@drawable/find_find_cell_right_head_image"
		android:layout_width="24.0dp"
		android:layout_height="24.0dp"
		app:layout_constraintEnd_toStartOf="@+id/right_icon"
		android:layout_marginEnd="10.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/circle"
		android:alpha="1"
		app:srcCompat="@drawable/find_find_cell_circle"
		app:layout_constraintStart_toStartOf="@+id/point_for_centerx_of_circle"
		android:layout_marginStart="0dp"
		app:layout_constraintEnd_toEndOf="@+id/point_for_centerx_of_circle"
		android:layout_marginEnd="0dp"
		app:layout_constraintTop_toTopOf="@+id/point_for_centery_of_circle"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="@+id/point_for_centery_of_circle"
		android:layout_marginBottom="0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/right_label"
		android:alpha="1"
		android:background="#00FFFFFF"
		android:text="@string/find_fragment_find_cell_right_label"
		android:fontFamily="sans-serif"
		android:textColor="#FF939396"
		android:textSize="12sp"
		android:gravity="end"
		app:layout_constraintEnd_toStartOf="@+id/right_head_image"
		android:layout_marginEnd="10.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

	<android.widget.Space
		android:id="@+id/point_for_centerx_of_circle"
		app:layout_constraintEnd_toEndOf="@+id/right_head_image"
		android:layout_marginEnd="0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="8.0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

	<android.widget.Space
		android:id="@+id/point_for_centery_of_circle"
		app:layout_constraintBottom_toTopOf="@+id/right_head_image"
		android:layout_marginBottom="0dp"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="8.0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>

