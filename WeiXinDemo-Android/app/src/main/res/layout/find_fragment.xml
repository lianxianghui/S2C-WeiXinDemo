<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/content_view"
	android:alpha="1"
	android:background="#FFEFEFF4"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<androidx.recyclerview.widget.RecyclerView
		android:id="@+id/list_view"
		android:alpha="1"
		android:background="#FFEFEFF4"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		app:layout_constraintTop_toBottomOf="@+id/custom_navigation_bar"
		android:layout_marginTop="15.0dp"
		android:layout_width="0dp"
		android:layout_height="0dp" />

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/custom_navigation_bar"
		android:alpha="1"
		android:background="#FF38363C"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		android:layout_height="45.0dp"
		android:layout_width="0dp">

		<android.view.View
			android:id="@+id/bottom_line"
			android:alpha="1"
			android:background="#FFB2B2B2"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="0dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="0dp"
			android:layout_height="0.5dp"
			android:layout_width="0dp" />

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/title"
			android:alpha="1"
			android:background="#00FFFFFF"
			android:text="@string/find_fragment_title"
			android:fontFamily="sans-serif-light"
			android:textColor="#FFFFFFFF"
			android:textSize="17sp"
			android:gravity="center_horizontal"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="0dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="0dp"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="0dp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />

	</androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

