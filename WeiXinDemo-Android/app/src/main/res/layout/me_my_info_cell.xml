<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/my_info_cell"
	android:alpha="1"
	android:background="@drawable/bg_normalcolor_ffffffff_pressedcolor_80ffffff"
	android:layout_height="80.0dp"
	android:layout_width="match_parent">

	<android.view.View
		android:id="@+id/bottom_line"
		android:alpha="1"
		android:background="#FFD8D8D8"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		android:layout_height="0.5dp"
		android:layout_width="0dp" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/right_icon"
		android:alpha="1"
		app:srcCompat="@drawable/me_me_my_info_cell_right_icon"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="15.0dp"
		android:layout_width="8.0dp"
		android:layout_height="13.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/label"
		android:alpha="1"
		android:background="#00FFFFFF"
		android:text="@string/me_my_info_cell_label"
		android:fontFamily="sans-serif"
		android:textColor="#FF9E9E9E"
		android:textSize="12sp"
		app:layout_constraintStart_toStartOf="@+id/name"
		android:layout_marginStart="0dp"
		app:layout_constraintBottom_toBottomOf="@+id/head_image"
		android:layout_marginBottom="4.4dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/name"
		android:alpha="1"
		android:background="#00FFFFFF"
		android:text="@string/me_my_info_cell_name"
		android:fontFamily="sans-serif"
		android:textColor="#FF1B1B1B"
		android:textSize="14sp"
		android:layout_height="23.0dp"
		app:layout_constraintStart_toEndOf="@+id/head_image"
		android:layout_marginStart="10.2dp"
		app:layout_constraintTop_toTopOf="@+id/point_for_bottom_of_name_to_top_of_right_image"
		android:layout_marginTop="0dp"
		android:layout_width="wrap_content" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/right_image"
		android:alpha="1"
		app:srcCompat="@drawable/me_me_my_info_cell_right_image"
		app:layout_constraintEnd_toEndOf="@+id/right_icon"
		android:layout_marginEnd="18.0dp"
		android:layout_width="18.0dp"
		android:layout_height="18.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/head_image"
		android:alpha="1"
		app:srcCompat="@drawable/me_me_my_info_cell_head_image"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="10.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

	<android.widget.Space
		android:id="@+id/point_for_bottom_of_name_to_top_of_right_image"
		app:layout_constraintBottom_toTopOf="@+id/right_image"
		android:layout_marginBottom="14.0dp"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="8.0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>

