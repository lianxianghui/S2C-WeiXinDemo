<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/content_view"
	android:alpha="1"
	android:background="#FFEFEFF4"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<androidx.recyclerview.widget.RecyclerView
		android:id="@+id/list_view"
		android:alpha="1"
		android:background="#FFEFEFF4"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		app:layout_constraintTop_toBottomOf="@+id/custom_navigation_bar"
		android:layout_marginTop="0dp"
		android:layout_width="0dp"
		android:layout_height="0dp" />

	<androidx.constraintlayout.widget.ConstraintLayout
		android:id="@+id/custom_navigation_bar"
		android:alpha="1"
		android:background="#FF38363C"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="0dp"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		android:layout_height="45.0dp"
		android:layout_width="0dp">

		<android.view.View
			android:id="@+id/bottom_line"
			android:alpha="1"
			android:background="#FFB2B2B2"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="0dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="0dp"
			android:layout_height="0.5dp"
			android:layout_width="0dp" />

		<androidx.constraintlayout.widget.ConstraintLayout
			android:id="@+id/right_button_container"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="0dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="0dp"
			android:layout_height="44.0dp"
			android:layout_width="60.0dp">

			<androidx.appcompat.widget.AppCompatButton
				android:id="@+id/right_button"
				android:alpha="1"
				android:background="@drawable/bg_normalcolor_00ffffff_pressedcolor_00ffffff"
				android:textAllCaps="false"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="0dp"
				app:layout_constraintEnd_toEndOf="parent"
				android:layout_marginEnd="0dp"
				app:layout_constraintTop_toTopOf="parent"
				android:layout_marginTop="0dp"
				app:layout_constraintBottom_toBottomOf="parent"
				android:layout_marginBottom="0dp"
				android:layout_width="0dp"
				android:layout_height="0dp" />

			<androidx.appcompat.widget.AppCompatImageView
				android:id="@+id/image"
				android:alpha="1"
				android:elevation="2dp"
				app:srcCompat="@drawable/contacts_contacts_fragment_image"
				android:layout_height="23.0dp"
				android:layout_width="25.3dp"
				app:layout_constraintTop_toTopOf="@+id/point_for_centery_of_image"
				android:layout_marginTop="0dp"
				app:layout_constraintBottom_toBottomOf="@+id/point_for_centery_of_image"
				android:layout_marginBottom="0dp"
				app:layout_constraintStart_toStartOf="@+id/point_for_centerx_of_image"
				android:layout_marginStart="0dp"
				app:layout_constraintEnd_toEndOf="@+id/point_for_centerx_of_image"
				android:layout_marginEnd="0dp" />

			<android.widget.Space
				android:id="@+id/point_for_centery_of_image"
				app:layout_constraintTop_toTopOf="@+id/point_for_centery_of_right_button"
				android:layout_marginTop="2.0dp"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="8.0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />

			<android.widget.Space
				android:id="@+id/point_for_centerx_of_right_button"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="0dp"
				app:layout_constraintEnd_toEndOf="parent"
				android:layout_marginEnd="0dp"
				app:layout_constraintTop_toTopOf="parent"
				android:layout_marginTop="8.0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />

			<android.widget.Space
				android:id="@+id/point_for_centerx_of_image"
				app:layout_constraintStart_toStartOf="@+id/point_for_centerx_of_right_button"
				android:layout_marginStart="2.0dp"
				app:layout_constraintTop_toTopOf="parent"
				android:layout_marginTop="8.0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />

			<android.widget.Space
				android:id="@+id/point_for_centery_of_right_button"
				app:layout_constraintTop_toTopOf="parent"
				android:layout_marginTop="0dp"
				app:layout_constraintBottom_toBottomOf="parent"
				android:layout_marginBottom="0dp"
				app:layout_constraintStart_toStartOf="parent"
				android:layout_marginStart="8.0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />

		</androidx.constraintlayout.widget.ConstraintLayout>

		<androidx.appcompat.widget.AppCompatTextView
			android:id="@+id/title"
			android:alpha="1"
			android:background="#00FFFFFF"
			android:text="@string/contacts_fragment_title"
			android:fontFamily="sans-serif-light"
			android:textColor="#FFF6F6F6"
			android:textSize="17sp"
			android:gravity="center_horizontal"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="0dp"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="0dp"
			app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="0dp"
			app:layout_constraintBottom_toBottomOf="parent"
			android:layout_marginBottom="0dp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />

	</androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

