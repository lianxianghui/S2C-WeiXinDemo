<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/login_status_cell"
	android:alpha="1"
	android:background="@drawable/bg_normalcolor_00ffffff_pressedcolor_00ffffff"
	android:layout_height="40.0dp"
	android:layout_width="match_parent">

	<androidx.appcompat.widget.AppCompatImageView
		android:id="@+id/status_image"
		android:alpha="1"
		app:srcCompat="@drawable/weixin_conversation_list_login_status_cell_status_image"
		app:layout_constraintStart_toStartOf="parent"
		android:layout_marginStart="10.0dp"
		android:layout_width="35.0dp"
		android:layout_height="28.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp" />

	<androidx.appcompat.widget.AppCompatTextView
		android:id="@+id/status_label"
		android:alpha="1"
		android:background="#00FFFFFF"
		android:text="@string/conversation_list_fragment_conversation_list_login_status_cell_status_label"
		android:fontFamily="sans-serif"
		android:textColor="#FF939396"
		android:textSize="12sp"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginEnd="10.0dp"
		app:layout_constraintStart_toEndOf="@+id/status_image"
		android:layout_marginStart="17.0dp"
		app:layout_constraintTop_toTopOf="parent"
		android:layout_marginTop="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		android:layout_marginBottom="0dp"
		android:layout_width="0dp"
		android:layout_height="wrap_content" />

</androidx.constraintlayout.widget.ConstraintLayout>

